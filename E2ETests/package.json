{
  "name": "a2j-e2e-testing",
  "version": "4.0.0",
  "description": "Protracter E2E tests with Cucumber BDD framwork",
  "homepage": "https://github.com/igniteram/protractor-cucumber-typescript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf typeScript/",
    "clean-build": "npm run clean && npm run build",
    "lint": "jslint '**/*.js'",
    "pretest": "npm run lint",
    "test": "protractor typeScript/config/config.js",
    "debug": "node --inspect=1337 --debug-brk --nolazy node_modules/cucumber/bin/cucumber-js --tags @debug --format json:./reports/report.json",
    "webdriver-update": "webdriver-manager update",
    "webdriver-start": "webdriver-manager start"
  },
  "author": "Xiao Lin <xiao.lin@avanade.com>",
  "devDependencies": {
    "@types/cucumber": "^4.0.4",
    "@types/node": "^10.7.1",
    "@types/selenium-webdriver": "^3.0.10",
    "all-contributors-cli": "^5.4.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cucumber": "^4.2.1",
    "cucumber-html-reporter": "^4.0.3",
    "jslint": "^0.12.0",
    "mkdirp": "^0.5.1",
    "protractor": "^5.4.0",
    "protractor-cucumber-framework": "^6.0.0",
    "rimraf": "^2.6.2",
    "ts-node": "^7.0.1",
    "typescript": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/Access2Justice/tree/feature415/setting_up_e2e_tests"
  },
  "dependencies": {
    "@types/chai": "^4.1.4",
    "request-promise": "^4.2.2"
  }
}
